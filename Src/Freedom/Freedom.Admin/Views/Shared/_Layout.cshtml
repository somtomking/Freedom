<!DOCTYPE html>
<html lang="en">
<head>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <meta charset="utf-8" />
  <title>后台管理</title>

  <meta name="description" content="This is page-header (.page-header &gt; h1)" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />

  <!-- bootstrap & fontawesome -->
  <link rel="stylesheet" href="~/Content/Ace/assets/css/bootstrap.min.css" />
  <link rel="stylesheet" href="~/Content/Ace/assets/css/font-awesome.min.css" />


  <!-- page specific plugin styles -->
  <link rel="stylesheet" href="~/Content/Ace/assets/css/prettify.css" />

  <!-- text fonts -->
  <link rel="stylesheet" href="~/Content/Ace/assets/css/ace-fonts.css" />

  <!-- ace styles -->
  <link rel="stylesheet" href="~/Content/Ace/assets/css/ace.min.css" id="main-ace-style" />

  <!--[if lte IE 9]>
    <link rel="stylesheet" href="~/Content/Ace/assets/css/ace-part2.min.css" />
  <![endif]-->
  <link rel="stylesheet" href="~/Content/Ace/assets/css/ace-skins.min.css" />
  <link rel="stylesheet" href="~/Content/Ace/assets/css/ace-rtl.min.css" />

  <!--[if lte IE 9]>
    <link rel="stylesheet" href="~/Content/Ace/assets/css/ace-ie.min.css" />
  <![endif]-->
  <!-- inline styles related to this page -->
  <!-- ace settings handler -->
  <script src="~/Content/Ace/assets/js/ace-extra.min.js"></script>

  <!-- HTML5shiv and Respond.js for IE8 to support HTML5 elements and media queries -->
  <!--[if lte IE 8]>
  <script src="~/Content/Ace/assets/js/html5shiv.min.js"></script>
  <script src="~/Content/Ace/assets/js/respond.min.js"></script>
  <![endif]-->
</head> 

<body class="no-skin">
  <!-- #section:basics/navbar.layout -->
  @Html.Partial("Navbar")

  <!-- /section:basics/navbar.layout -->
  <div class="main-container" id="main-container">
    <script type="text/javascript">
      try { ace.settings.check('main-container', 'fixed') } catch (e) { }
    </script>

    <!-- #section:basics/sidebar -->
    @Html.Partial("Menu")

    <!-- /section:basics/sidebar -->
    <div class="main-content">
      <!-- #section:basics/content.breadcrumbs -->
      @Html.Partial("Breadcrumbs")

      <!-- /section:basics/content.breadcrumbs -->
      <div class="page-content">
        <!-- #section:settings.box -->
        @Html.Partial("SettingBox")
        <!-- /section:settings.box -->
        <div class="page-content-area">

          @RenderBody()

        </div><!-- /.page-content-area -->
      </div><!-- /.page-content -->
    </div><!-- /.main-content -->

    <div class="footer">
      <div class="footer-inner">
        <!-- #section:basics/footer -->
        <div class="footer-content">
          <span class="bigger-120">
            <span class="blue bolder">Ace</span>
            Application &copy; 2013-2014
          </span>

          &nbsp; &nbsp;
          <span class="action-buttons">
            <a href="#">
              <i class="ace-icon fa fa-twitter-square light-blue bigger-150"></i>
            </a>

            <a href="#">
              <i class="ace-icon fa fa-facebook-square text-primary bigger-150"></i>
            </a>

            <a href="#">
              <i class="ace-icon fa fa-rss-square orange bigger-150"></i>
            </a>
          </span>
        </div>

        <!-- /section:basics/footer -->
      </div>
    </div>

    <a href="#" id="btn-scroll-up" class="btn-scroll-up btn btn-sm btn-inverse">
      <i class="ace-icon fa fa-angle-double-up icon-only bigger-110"></i>
    </a>
  </div><!-- /.main-container -->
  <!-- basic scripts -->
  <!--[if !IE]> -->
  <script type="text/javascript">
    window.jQuery || document.write("<script src='/Content/Ace/assets/js/jquery.min.js'>" + "<" + "/script>");
  </script>

  <!-- <![endif]-->
  <!--[if IE]>
  <script type="text/javascript">
   window.jQuery || document.write("<script src='~/Content/Ace/assets/js/jquery1x.min.js'>"+"<"+"/script>");
  </script>
  <![endif]-->
  <script type="text/javascript">
    if ('ontouchstart' in document.documentElement) document.write("<script src='/Content/Ace/assets/js/jquery.mobile.custom.min.js'>" + "<" + "/script>");
  </script>
  <script src="~/Content/Ace/assets/js/bootstrap.min.js"></script>

  <!-- page specific plugin scripts -->
  <script src="~/Content/Ace/assets/js/prettify.js"></script>

  <!-- ace scripts -->
  <script src="~/Content/Ace/assets/js/ace-elements.min.js"></script>
  <script src="~/Content/Ace/assets/js/ace.min.js"></script>

  <!-- inline scripts related to this page -->
  <script type="text/javascript">
    jQuery(function ($) {

      window.prettyPrint && prettyPrint();
      $('#id-check-horizontal').removeAttr('checked').on('click', function () {
        $('#dt-list-1').toggleClass('dl-horizontal').prev().html(this.checked ? '&lt;dl class="dl-horizontal"&gt;' : '&lt;dl&gt;');
      });

    })
  </script>


</body>
</html>
